                    <RowDefinition Height="*"/>
                    <RowDefinition Height="2*"/>
                    <RowDefinition Height="2*"/>
                    <RowDefinition Height="2*"/>
                </Grid.RowDefinitions>
                <Button Grid.Column="0" Grid.Row="0" Name="btnFacSearch" Content="Search" FontWeight="Bold" Style="{StaticResource flatbutton}"
